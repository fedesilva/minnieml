
let out = mkBufferWithSize (1024 * 4 * 10);

fn loop(n: Int, t: Int): Unit =
  if n <= t then
    if n % 15 == 0 then out writeln "FizzBuzz"
    elif n % 3 == 0 then out writeln "Fizz"
    elif n % 5 == 0 then out writeln "Buzz"
    else out writeln_int n
    end; 
    loop (n+1) t
  end
;

fn fizzbuzz(n: Int) = loop 1 n;

fn main() =  
  fizzbuzz 10000000;
  flush out
;
