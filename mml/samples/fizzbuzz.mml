
fn loop(n: Int, t: Int): Unit =
  if n <= t then
    if n % 15 == 0 then println "FizzBuzz"
    elif n % 3 == 0 then println "Fizz"
    elif n % 5 == 0 then println "Buzz"
    else println (to_string n)
    end;
    loop (n+1) t
  end
;

fn fizzbuzz(n: Int) = loop 1 n;

fn main() =  
  fizzbuzz 10000000
;
