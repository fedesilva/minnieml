
fn make_test_results(): String =
  "Zero: "      ++ (int_to_str 0)   ++ ", " ++
  "Positive: "  ++ (int_to_str 123) ++ ", " ++
  "Large: "     ++ (int_to_str 1234567890)
;

fn loop(i: Int, to: Int): Unit = 
  if i <= to then
    println (make_test_results ());
    loop (i+1) to
  end
;
  

fn main(): Unit =
  loop 1 1000
;

