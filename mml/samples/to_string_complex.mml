fn make_test_results(): String = 
  concat 
    (concat "Zero: " (concat (to_string 0) ", "))
    (concat 
      (concat "Positive: " (concat (to_string 123) ", "))
      (concat "Large: " (to_string 1234567890))
    );

fn loop(i: Int, to: Int): Unit = 
  if i <= to then
    println (make_test_results ());
    loop (i+1) to
  end
;
  

fn main(): Unit =
  loop 1 1000
;
